{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "target": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "strictFunctionTypes": true,
    "module": "ES2022",
    "lib": [
      "DOM",
      "WebWorker"
    ]
  }
}